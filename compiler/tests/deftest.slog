 (next EA EA_jump)
 (direct_jump EA_jump EA_target)
 (instruction EA_jump _ _ Operation _ _ _ _ _ _)
 (arch.jump_equal_operation Operation)]

[(flow_def EA_jump Reg EA_target Immediate)
