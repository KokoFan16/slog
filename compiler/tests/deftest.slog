 (code EA)
 (cmp_immediate_to_reg  EA Reg _ Immediate)
 (next EA EA_jump)
 (direct_jump EA_jump _)
 (next EA_jump EA_target)
 (instruction EA_jump _ _ Operation _ _ _ _ _ _)
 (arch.jump_unequal_operation Operation)]

[(must_def EA Reg)
 <--
