;; F ------------------------

(comp ==)
[(= x y) --> (== x y)]

(f-foo 0)
(f-foo 1)
(f-foo 2)
(f-bar 1 3)
(f-bar 2 0)
(f-bar 1 2)
(f-bar 2 4)

[(f-foo x)
 (or (= id (f-bar x {+ x 2})) 
     (= id (f-bar {+ x 2} {== x})))
 -->
 (f-baz id)]

